<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="whottadota.DeleteTournamentWindow"
        Title="Удалить турнир"
        Background="Black"
      >
    <Grid>
        <TextBlock Foreground="Aqua" Text="Удаление" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,10,0,0" />
        <TextBlock Foreground="Aqua" Text="Подтвердить удаление?" FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="20,0,20,40" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,20">
            <Button Background="White" Foreground="Pink" BorderBrush="Aqua" BorderThickness="3"  Name = "Yes_Btn" Content="Да" Click="Yes_Btn_OnClick" />
            <Button Background="White" Foreground="Pink" BorderBrush="Aqua" BorderThickness="3"  Name = "No_Btn" Content="Нет" Click="No_Btn_OnClick" />
        </StackPanel>
    </Grid>
</Window>
